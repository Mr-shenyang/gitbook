{
  "language": "zh-hans",
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "chapter-fold",
    "splitter",
    "code",
    "todo",
    "back-to-top-button",
    "-sharing",
    "sharing-plus",
    "pageview-count",
    "mermaid-gb3",
    "new-flowchart",
    "prism",
    "prism-themes",
    "advanced-emoji"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-xonokai.css"
      ],
      "lang": {
        "flow": "typescript"
      },
      "ignore": [
        "mermaid",
        "mermaid-gb3"
      ]
    },
    "sharing": {
      "all": [
        "weibo",
        "qq",
        "qzone",
        "whatsapp"
      ]
    }
  }
}